{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schemas/nav/nav_poi_result.schema.json",
  "title": "NavPOIResult",
  "type": "object",
  "additionalProperties": false,
  "required": ["items"],
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["name", "lat", "lon"],
        "properties": {
          "name": { "type": "string" },
          "lat": { "type": "number" },
          "lon": { "type": "number" },
          "address": { "type": "string" },
          "distance_m": { "type": "number", "minimum": 0 }
        }
      }
    }
  }
}
